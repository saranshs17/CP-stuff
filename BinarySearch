#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int ans=-1;
    int search;
    cin>>search;
    sort(arr,arr+n);
    int lo=0,hi=n-1,mid;
    cout<<"Sorted Array: ";
    for(int x:arr)
    {
        cout<<x<<" ";
    }
    cout<<'\n';
    while(hi>=lo)
    {
        mid=lo+(hi-lo)/2;
        if(arr[mid]==search)
        {
            ans=mid;
            cout<<"index of "<<search<<" is "<<ans;
            break;
        }
        else if(arr[mid]<search)
        {
            lo=mid+1;
        }
        else if(arr[mid]>search)
        {
            hi=mid-1;
        }
    }
}
